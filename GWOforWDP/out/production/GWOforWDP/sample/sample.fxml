            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </Label>
    <Button fx:id="btnSolve" layoutX="277.0" layoutY="14.0" mnemonicParsing="false" onAction="#solveButton" prefHeight="50.0" prefWidth="230.0" text="Solve" textAlignment="CENTER" />
